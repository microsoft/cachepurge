{

  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "samplingExcludedTypes": "Request",
        "isEnabled": true
      }
    },
    "fileLoggingMode": "always",
    "console": { "isEnabled": "false" },
    "logLevel": {
      "default": "Debug"
    }
  },
  "queues": {
    "maxPollingInterval": 60000
  }
}